"VertexlitGeneric"
{
	"$baseTexture" "models/weapons/c_models/c_tranq/c_revolver"
	"$phongexponenttexture" "models/weapons/c_models/c_tranq/c_revolver_exp"
	"$bumpmap" "models/weapons/c_models/c_tranq/c_revolver_normal"
	"$phong" "1"
	//commented out to use the exponent mask
	//"$phongexponent" "25"
	"$phongboost" ".3"	
	"$phongalbedotint" "1"
	
	//scales up the exponent texture which usually only has about 120 levels of exponent
	"$phongexponentfactor"  "90"
	//"$phongalbedotint" "0"
	
	"$lightwarptexture" "models\weapons\c_models\c_tranq\c_revolver_lightwarp"			
	"$phongfresnelranges"	"[5 5.5 10]"
	"$halflambert" "1"
	"$glowcolor" "1"

	"$tempvar" "0"

	//commented out to use the exponent mask
	//"$basephongexponent" "15"
	//"$basephongboost" "3"

	//"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent" "1"				
	"$rimlightboost" "1.5"
	"$rimmask" "1"
	

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
	
		"WeaponSkin" {
		}
	}
}